{
  "displayName": "Job duration in PENDING status for 10 minutes",
  "documentation": {
    "content": "This alert indicates that the job is in pending status for 10 minutes. If you want the alert to fire when the job is in pending status for 20 minutes, set the threshold value to 1200s.",
    "mimeType": "text/markdown"
  },
  "userLabels": {
    "resource": "dataproc-jobs"
  },
  "conditions": [
    {
      "displayName": "Job duration in PENDING status for 10 minutes",
      "conditionAbsent": {
        "filter": "metric.type=\"dataproc.googleapis.com/cluster/job/duration\" AND resource.type=\"cloud_dataproc_cluster\" AND metric.labels.state=\"PENDING\"",
        "duration": "600s",
        "trigger": {
          "count": 1
        },
        "aggregations": [
          {
            "alignmentPeriod": "60s",
            "crossSeriesReducer": "REDUCE_SUM",
            "perSeriesAligner": "ALIGN_SUM"
          }
        ]
      }
    }
  ],
  "alertStrategy": {
    "autoClose": "86400s",
    "notificationPrompts": [
      "OPENED",
      "CLOSED"
    ]
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": [],
  "severity": "WARNING"
}